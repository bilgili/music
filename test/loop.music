np=1
stoptime=1.1
[A]
  binary=../utils/eventdelay
  args=-d 0.1 -L 1 -b 2
[B]
  binary=../utils/eventdelay
  args=-d 0.1 -L 2 -b 2
[from]
  binary=/usr/local/bin/eventsource
  args=-b 5 10 spikes
[to]
  binary=/usr/local/bin/eventlogger
  args=-b 5


from.out -> A.in [10]
A.out -> B.in [10]
B.out -> A.aux [10]
B.out -> to.in [10]
